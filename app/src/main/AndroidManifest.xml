<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tuitam">

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".KalendarzActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".SendEmailActivity"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".OfertaActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".KontaktActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".GaleriaActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".OnasActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".HomeActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>